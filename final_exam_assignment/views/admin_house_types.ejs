<% include partials/admin_head.ejs %>
<% include partials/admin_header.ejs %>
<main role="main">
    <% include partials/admin_nav.ejs %>
    <section>
        <a class="newData" href="/admin/house_type/new">Opret en ny bolig type</a>
        <table>
            <tr>
                <th>ID</th>
                <th>Navn</th>
                <th>Rediger / Slet</th>
            </tr>
            <% results.forEach(result => { %>
            <tr>
                <td>
                    <%= result.id %>
                </td>
                <td>
                    <%= result.name %>
                </td>
                <td>
                    <div>
                        <a href="/admin/house_type/edit/<%= result.id %>">
                            <p>Rediger</p><i class="fas fa-edit"></i>
                        </a>
                        <hr>
                        <a href="/admin/house_type/delete/<%= result.id %>">
                            <p>Slet</p><i class="fas fa-trash"></i>
                        </a>
                    </div>
                </td>
            </tr>
            <% }); %>
        </table>
    </section>
</main>

<% include partials/admin_footer.ejs %>